
  <%=form_for @video,:url => spud_admin_video_path(:id => @video.id),:html=>{:class=>"form-horizontal"} do |f|%>
    <%=render :partial => "form",:locals => {:f => f}%>
	<div class="form-actions">
    	<%=f.submit "Save Video", :class=>"btn btn-primary"%> or <%=link_to "cancel",spud_admin_videos_path,:class => "btn"%>
    </div>

  <%end%>
